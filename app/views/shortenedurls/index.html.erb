<h1>All shortened urls</h1>

<table>
  <thead>
    <tr>
      <th>Original</th>
      <th>Shortened</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @shortenedurls.each do |shortenedurl| %>
      <tr>
        <td><%= shortenedurl.original %></td>
        <td><%= shortenedurl.shortened %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

